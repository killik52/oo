        <activity
            android:name=".InstrucoesPagamentoActivity"
            android:exported="false" />
        <activity
            android:name=".InformacoesEmpresaActivity"
            android:exported="false" />
        <activity
            android:name=".DefinicoesActivity"
            android:exported="false" />
        <activity
